<?xml version="1.0"?>
<!DOCTYPE look-and-feel PUBLIC "-//Liferay//DTD Look and Feel 7.0.0//EN" "http://www.liferay.com/dtd/liferay-look-and-feel_7_0_0.dtd">

<look-and-feel>
	<compatibility>
		<version>7.0.0+</version>
	</compatibility>
	<theme id="ventana-theme" name="Ventana theme">
		<template-extension>ftl</template-extension>
		<settings>
			<setting configurable="true" key="espacio-personal-enlace" type="text" />
			<setting configurable="true" key="url-login" type="text" />
			<setting configurable="true" key="url-logout" type="text" />
			<setting configurable="true" key="enlaces-pie-article-id" type="text" />
			<setting configurable="true" key="ventana-group-id" type="text" />
			<setting configurable="true" key="twitter-link" type="text" />
			<setting configurable="true" key="facebook-link" type="text" />
			<setting configurable="true" key="instagram-link" type="text" />
			<setting configurable="true" key="pinterest-link" type="text" />
			<setting configurable="true" key="url-consejeria" type="text" />
			<setting configurable="true" key="url-junta" type="text" />
			<setting configurable="true" key="url-feder" type="text" />
			<setting configurable="true" key="url-ventana" type="text" />
			<setting configurable="true" key="url-cookies" type="text" />
		</settings>
		<portlet-decorator id="barebone" name="Barebone">
			<portlet-decorator-css-class>portlet-barebone</portlet-decorator-css-class>
		</portlet-decorator>
		<portlet-decorator id="borderless" name="Borderless">
			<portlet-decorator-css-class>portlet-borderless</portlet-decorator-css-class>
		</portlet-decorator>
		<portlet-decorator id="decorate" name="Decorate">
			<default-portlet-decorator>true</default-portlet-decorator>
			<portlet-decorator-css-class>portlet-decorate</portlet-decorator-css-class>
		</portlet-decorator>

	</theme>
</look-and-feel>
